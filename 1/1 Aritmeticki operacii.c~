#include <stdio.h>

int main () {
    // deklaracija na dve celobrojni promelivi
    int a, b;
    // chitanje na dva broja od standarden vlez
	scanf("%d %d" , &a, &b) ;
	printf("a + b = %d\n", a + b);
	printf("a - b = %d\n", a - b);
	printf("a * b = %d\n", a * b);
	printf("a / b = %d\n", a / b);
	printf("a % b = %d\n", a % b);
	// vasiot kod ovde
    
    // kraj na vasiot kod 
    return 0;
}
